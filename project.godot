; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Vacuum"
run/main_scene="res://Menu/Scena_menu.tscn"
boot_splash/image="res://splash.png"
config/icon="res://Ambiente/Immagini/Nuvole.png"

[display]

window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/shrink="1"

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/subsurface_scattering/follow_surface=true
environment/default_clear_color=Color( 0.202179, 0.64987, 0.976563, 1 )
environment/default_environment="res://default_env.tres"
